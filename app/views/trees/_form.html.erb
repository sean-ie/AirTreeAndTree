<%= simple_form_for [ @tree ] do |f| %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :address %>
<%= f.input :price %>
<%= f.input :number_of_guests %>
<%= f.input :photo, as: :file, input_html: { multiple: true } %> 
<%= f.submit "Submit", class: "btn btn-success" %>
<% end %>