<div class="card-show">

<h1>Book your Treehouse</h1>
<%= simple_form_for [ @tree, @booking ] do |f| %>
<%= f.input :start_date, as: :date, html5:true %>
<%= f.input :end_date, as: :date, html5: true %>
<%= f.submit "Submit", class: "btn btn-success" %>
<% end %>

<%= link_to "Back", trees_path, class: "btn btn-success" %>

</div>

